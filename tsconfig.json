{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "paths": {
      "/@assets/*": ["./src/assets/*"],
      "/@components/*": ["./src/components/*"],
      "/@composables/*": ["./src/composables/*"],
      "/@lib/*": ["./src/lib/*"],
      "/@pages/*": ["./src/pages/*"],
      "/@types/*": ["./src/types/*"],
      "/@src/*": ["./src/*"],
    }
  }
}
