{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "module": "esnext",
    "target": "esnext",
    "paths": {
      "/@assets/*": ["./src/assets/*"],
      "/@components/*": ["./src/components/*"],
      "/@composables/*": ["./src/composables/*"],
      "/@utils/*": ["./src/utils/*"],
      "/@pages/*": ["./src/pages/*"],
      "/@types/*": ["./src/types/*"]
      // "/@src/*": ["./src/*"],
    }
  }
}
